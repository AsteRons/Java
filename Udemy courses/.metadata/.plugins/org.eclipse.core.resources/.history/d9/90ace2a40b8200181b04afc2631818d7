{
	
	public Main()
	{
		super("Uk³ad graficzny (Layout)");			// Tytu³
		
		this.setBounds(300, 320, 300, 200);			// Ustawiamy granice
		
	}
	
	
	public static void main(String[] args) {

		new Main().setVisible(true);
		
	}

}
